class Solution {
    public int sum(int num1, int num2) {
        return num1 + num2;
    }

    public static void main(String[] args) {
        Solution solution = new Solution();
        
        // Example 1
        int num1 = 12, num2 = 5;
        System.out.println("Output: " + solution.sum(num1, num2));

        // Example 2
        num1 = -10;
        num2 = 4;
        System.out.println("Output: " + solution.sum(num1, num2));
    }
}
